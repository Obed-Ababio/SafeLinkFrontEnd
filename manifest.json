{
	"name": "SafeLink",
	"description": 
	"an ext for detecting suspicious links within web pages",
	"version": "0.1.0",
	"manifest_version": 2,
	"icons": {

	},
	"background": {
		"scripts": ["./background.js"]
	},
	"options_page": "./options.html",
	"browser_action": {
		"default_popup": "popup.html",
		"default_title": "SafeLink"
	},
	"permissions" : [
		"tabs", 
		"storage",
		"http://*/",
        "https://*/"
	]
}